<?xml version="1.0" encoding="UTF-8"?>
<testsuites>
	<testsuite>
		<% report.forEach(function (testCase) { %>
			<testcase name="<%= testCase.name %>">
				<% testCase.failures.forEach(function (failure) { %>
					<failure message="<%= failure %>" />
				<%}); %>
			</testcase>
		<% }); %>
	</testsuite>
</testsuites>